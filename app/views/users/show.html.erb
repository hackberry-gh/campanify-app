<% if me? %>
<nav>
	<%= link_to "profile", edit_user_registration_path %>
</nav>
<% else %>
<script>
$.post("<%= visits_user_path(user) %>", {_method:"PUT",action:"visits"}, "json");
</script>
<% end %>
<article class="user">
	<%= user.first_name %>
	<br/>
	visits: <%= user.total_visits %><br/>
	recruits: <%= user.total_recruits %>	
</article>	